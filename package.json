    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "gzip-size": "^3.0.0",
    "jest": "^20.0.4",
    "pretty-bytes": "^4.0.2",
    "react": "^15.6.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
